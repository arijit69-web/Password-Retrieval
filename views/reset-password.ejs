    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Reset Password</title>
  </head>
  <body>
    <h1 style="color:blue;">
        Reset Password 
      </h1>
  
      <div style="padding-top: 100px;">
        <form action="" method="POST">
            <div class="mb-3" style="padding-left: 10px;">
                <label for="password" class="form-label">Enter New Password</label>
                <input style="width:200px;" type="password" name="password"  class="form-control" id="password" />
             
            <div class="mb-3">
                <label for="conpassword" class="form-label">Re-Enter New Password</label>
                <input style="width:200px; " type="password" class="form-control" name="conpassword" id="conpassword" />
            </div>
           
            <button type="submit" class="btn btn-primary">Change Password</button>
          </form>
      </div>
   
   
  </body>
</html>
